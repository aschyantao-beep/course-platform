{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/courses/(.*)",
        "dest": "/courses/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/admin/(.*)",
        "dest": "/admin/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }